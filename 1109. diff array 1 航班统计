class Solution:
    def corpFlightBookings(self, bookings: List[List[int]], n: int) -> List[int]:
        
        diff_a = [0] * (n+1)
        
        for s,e,v in bookings:
            diff_a[s-1]+=v
            if e < n:
                diff_a[e]-=v
            
        res = [0] * n
        res[0] = diff_a[0]
        for i in range(1, n):
            res[i] = res[i-1] + diff_a[i]
        
        return res